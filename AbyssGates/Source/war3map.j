    if ( ( udg_itemDatabaseType[R2I(GetWidgetLife(GetManipulatedItem()))] == 4 ) or ( udg_itemDatabaseType[R2I(GetWidgetLife(GetManipulatedItem()))] == 5 ) or ( udg_itemDatabaseType[R2I(GetWidgetLife(GetManipulatedItem()))] == 6 ) ) then
        set udg_HeroPrimaryHandSlot[( 1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) )]=false
        set udg_HeroSecondaryHandSlot[( 1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) )]=false
        call DisplayTimedTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), 2., "Primary hand slot open")
        call DisplayTimedTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), 2., "Second hand slot opened")
    endif
    if ( udg_itemDatabaseType[R2I(GetWidgetLife(GetManipulatedItem()))] == 7 ) then
        set udg_HeroSecondaryHandSlot[( 1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) )]=false
        call DisplayTimedTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), 2., "Second hand slot opened")
    endif
    if ( ( udg_itemDatabaseType[R2I(GetWidgetLife(GetManipulatedItem()))] == 8 ) or ( udg_itemDatabaseType[R2I(GetWidgetLife(GetManipulatedItem()))] == 9 ) or ( udg_itemDatabaseType[R2I(GetWidgetLife(GetManipulatedItem()))] == 10 ) ) then
        if ( udg_itemDatabaseType[R2I(GetWidgetLife(GetManipulatedItem()))] == 9 ) then
            call UnitRemoveAbility(GetTriggerUnit(), 'A033')
        endif
        set udg_HeroArmorSlot[( 1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) )]=false
        call DisplayTimedTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), 2., "Armor slot opened")
    endif
endfunction

