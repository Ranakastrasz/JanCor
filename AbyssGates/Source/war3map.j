function Trig_PVPR_Boss_Lobster_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    if ( GetOwningPlayer(GetDyingUnit()) == Player(11) ) then
        set udg_TempInt=1
    else
        set udg_TempInt=5
    endif
    set bj_forLoopAIndex=udg_TempInt
    set bj_forLoopAIndexEnd=( udg_TempInt + 3 )
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call SetUnitManaBJ(udg_units07[bj_forLoopAIndex], ( GetUnitStateSwap(UNIT_STATE_MANA, udg_units07[bj_forLoopAIndex]) + 12. ))
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    if ( udg_TempInt == 1 ) then
        call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS, 8., "Makrai downed, +12 mana to North Side PVP!")
    endif
    if ( udg_TempInt == 5 ) then
        call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS, 8., "Makrai downed, +12 mana to South Side PVP!")
    endif
endfunction

