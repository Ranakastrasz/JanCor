function Trig_Set_Special_Effect_on_Gain_Actions takes nothing returns nothing
    call AdjustPlayerStateBJ(1, GetOwningPlayer(GetTriggerUnit()), PLAYER_STATE_RESOURCE_LUMBER)
    call DestroyEffect(udg_effects05[( 1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) )])
    call AddSpecialEffectTargetUnitBJ("overhead", GetTriggerUnit(), "Abilities\\Spells\\Other\\TalkToMe\\TalkToMe.mdl")
    set udg_effects05[( 1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) )]=bj_lastCreatedEffect
    if ( GetHeroStatBJ(0, GetTriggerUnit(), false) > 150 ) then
        call ModifyHeroStat(0, GetTriggerUnit(), 2, 150)
    endif
    if ( GetHeroStatBJ(1, GetTriggerUnit(), false) > 180 ) then
        call ModifyHeroStat(1, GetTriggerUnit(), 2, 180)
    endif
    if ( ( GetHeroLevel(GetTriggerUnit()) == 2 ) and ( udg_HelpBool[( 1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) )] ) ) then
        call DisplayTimedTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), 20., "|cff33cc88          After gaining a level your \"ability\" button will become bright red. Click on it to select a new ability, or to upgrade an old one. A exclamation will appear above your units head if you have any skill points left to spend.|r")
    endif
    if ( ( GetHeroLevel(GetTriggerUnit()) == 5 ) and ( udg_HelpBool[( 1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) )] ) ) then
        call DisplayTimedTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), 20., "|cff33cc88          Do NOT let unique units (bosses) through! If a unique enters their goal, your team will lose instantly.|r")
    endif
    if ( ( GetHeroLevel(GetTriggerUnit()) == 10 ) and ( udg_HelpBool[( 1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) )] ) ) then
        call DisplayTimedTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), 20., "|cff33cc88         More powerful items can be purchase at the shops up top.  It will be neccesary to purchase these to survive in later rounds.|r")
    endif
    if ( ( GetHeroLevel(GetTriggerUnit()) == 18 ) and ( udg_HelpBool[( 1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) )] ) ) then
        call DisplayTimedTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), 20., "|cff33cc88         Don't forget about the artifact shop near spawn. It contains powerful (although very expensive) weapons and armor. Additional lumber required for items can be obtained from leveling or from the magic specialties shop up north.|r")
    endif
    if ( ( GetHeroLevel(GetTriggerUnit()) == 20 ) and ( udg_HelpBool[( 1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) )] ) ) then
        call DisplayTimedTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), 20., "|cff33cc88         Help messages can be disabled at any point in the game by entering the command \"-helpoff\"|r")
    endif
endfunction

