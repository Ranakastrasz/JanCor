    return ( IsUnitAliveBJ(GetFilterUnit()) and ( GetUnitTypeId(GetFilterUnit()) == 'u005' ) )
endfunction

function Trig_Acquire_Creature_Heart_Actions takes nothing returns nothing
 local unit u= GetTriggerUnit()
    local player owner= GetOwningPlayer(u)
 local item itm= GetManipulatedItem()
    set udg_TempInt=0
    if ( GetUnitTypeId(u) == 'u005' ) then
        if ( GetItemTypeId(GetManipulatedItem()) == 'I00Z' ) then
            call AddSpecialEffectTargetUnitBJ("weapon", u, "Environment\\LargeBuildingFire\\LargeBuildingFire1.mdl") // Leak, Use weapon dummy sfx instead
            call AddSpecialEffectTargetUnitBJ("origin", u, "Abilities\\Spells\\Human\\ReviveHuman\\ReviveHuman.mdl")
            call ConditionalTriggerExecute(udg_trigger_DestroyFX)
        endif
        if ( GetItemTypeId(GetManipulatedItem()) == 'I010' ) then
            call AddSpecialEffectTargetUnitBJ("origin", u, "Abilities\\Spells\\Undead\\FrostNova\\FrostNovaTarget.mdl")
            call ConditionalTriggerExecute(udg_trigger_DestroyFX)
        endif
        if ( GetItemTypeId(GetManipulatedItem()) == 'I01C' ) then
            call AddSpecialEffectTargetUnitBJ("origin", u, "Abilities\\Spells\\Undead\\DeathCoil\\DeathCoilSpecialArt.mdl")
            call ConditionalTriggerExecute(udg_trigger_DestroyFX)
            call AddSpecialEffectTargetUnitBJ("weapon", u, "Abilities\\Spells\\Human\\Banish\\BanishTarget.mdl")
        endif
    endif
    if ( GetUnitTypeId(u) == 'H00A' ) then
        if ( UnitInventoryCount(udg_units05[( 1 + GetPlayerId(owner) )]) == 1 ) then
            set udg_TempInt=1
            call PolledWait(.05)
            call CreateItemLoc(GetItemTypeId(GetManipulatedItem()), GetUnitLoc(u))
            set udg_item01=bj_lastCreatedItem
            call SetWidgetLife(GetManipulatedItem(), .0)
            call AdjustPlayerStateBJ(( udg_itemDatabaseGold[udg_integer02] / 10 ), owner, PLAYER_STATE_RESOURCE_GOLD)
            call DisplayTimedTextToForce(GetForceOfPlayer(owner), 3., "Your creature already has an item! Item sold for 100% of value.")
            call RemoveItem(GetManipulatedItem())
            call StartSoundForPlayerBJ(owner, gg_snd_Error)
            set udg_TargetUnit=u
            call ConditionalTriggerExecute(udg_trigger53)
        
        endif
        if ( CountUnitsInGroup(GetUnitsOfPlayerMatching(owner, Condition(function Trig_Acquire_Creature_Heart_Filter))) == 1 ) and ( udg_TempInt == 0 ) then
            call PolledWait(.05)
            call UnitAddItem(udg_units05[( 1 + GetPlayerId(owner) )], GetManipulatedItem())
        endif
    endif
    
    if ( ( GetUnitTypeId(u) != 'u005' ) and ( GetUnitTypeId(u) != 'H00A' ) and ( GetUnitTypeId(u) != 'h012' ) ) then
        call PolledWait(.05)
        call CreateItemLoc(GetItemTypeId(GetManipulatedItem()), GetUnitLoc(GetTriggerUnit()))
        call SetWidgetLife(GetManipulatedItem(), .0)
        call RemoveItem(GetManipulatedItem())
        call DisplayTimedTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), 3., "This item may only be used by the Grim Magus's Creature")
        call StartSoundForPlayerBJ(owner, gg_snd_Error)
    endif
endfunction

