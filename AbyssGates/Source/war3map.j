function Trig_ITMSPL_Teleport_Damage_Actions takes nothing returns nothing
    set udg_location02=GetUnitLoc(GetTriggerUnit())
    set udg_TempInt=( 1 + GetPlayerId(udg_TempPlayer) )
    set udg_TempPlayer=GetOwningPlayer(GetTriggerUnit())
    set udg_location01=PolarProjectionBJ(udg_location02, 120., 0)
    call GroupClear(udg_group01)
    call GroupAddGroup(GetUnitsInRangeOfLocMatching(120., udg_location01, Condition(function Trig_ITMSPL_Teleport_Damage_Filter)), udg_group01)
    if ( ( CountUnitsInGroup(udg_group01) > 0 ) and ( GetUnitStateSwap(UNIT_STATE_MANA, udg_Hero[udg_TempInt]) >= 20. ) ) then
        set udg_TargetUnit=GroupPickRandomUnit(udg_group01)
        call CreateNUnitsAtLoc(1, 'u00U', udg_TempPlayer, udg_location02, bj_UNIT_FACING)
        call IssueTargetOrderById(bj_lastCreatedUnit, 852119, udg_TargetUnit)
        call UnitApplyTimedLifeBJ(5., 'BTLF', bj_lastCreatedUnit)
        call SetUnitManaBJ(udg_Hero[udg_TempInt], ( GetUnitStateSwap(UNIT_STATE_MANA, udg_Hero[udg_TempInt]) - 50. ))
    endif
    call GroupClear(udg_group01)
    set udg_location01=PolarProjectionBJ(udg_location02, 120., 120.)
    call GroupAddGroup(GetUnitsInRangeOfLocMatching(120., udg_location01, Condition(function Trig_ITMSPL_Teleport_Damage_Filter)), udg_group01)
    if ( ( CountUnitsInGroup(udg_group01) > 0 ) and ( GetUnitStateSwap(UNIT_STATE_MANA, udg_Hero[udg_TempInt]) >= 20. ) ) then
        set udg_TargetUnit=GroupPickRandomUnit(udg_group01)
        call CreateNUnitsAtLoc(1, 'u00U', udg_TempPlayer, udg_location02, bj_UNIT_FACING)
        call IssueTargetOrderById(bj_lastCreatedUnit, 852119, udg_TargetUnit)
        call UnitApplyTimedLifeBJ(5., 'BTLF', bj_lastCreatedUnit)
        call SetUnitManaBJ(udg_Hero[udg_TempInt], ( GetUnitStateSwap(UNIT_STATE_MANA, udg_Hero[udg_TempInt]) - 50. ))
    endif
    call GroupClear(udg_group01)
    set udg_location01=PolarProjectionBJ(udg_location02, 120., 240.)
    call GroupAddGroup(GetUnitsInRangeOfLocMatching(120., udg_location01, Condition(function Trig_ITMSPL_Teleport_Damage_Filter)), udg_group01)
    if ( ( CountUnitsInGroup(udg_group01) > 0 ) and ( GetUnitStateSwap(UNIT_STATE_MANA, udg_Hero[udg_TempInt]) >= 20. ) ) then
        set udg_TargetUnit=GroupPickRandomUnit(udg_group01)
        call CreateNUnitsAtLoc(1, 'u00U', udg_TempPlayer, udg_location02, bj_UNIT_FACING)
        call IssueTargetOrderById(bj_lastCreatedUnit, 852119, udg_TargetUnit)
        call UnitApplyTimedLifeBJ(5., 'BTLF', bj_lastCreatedUnit)
        call SetUnitManaBJ(udg_Hero[udg_TempInt], ( GetUnitStateSwap(UNIT_STATE_MANA, udg_Hero[udg_TempInt]) - 50. ))
    endif
    call GroupClear(udg_group01)
endfunction

