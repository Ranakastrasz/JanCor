function Trig_Init_STARTGAME_Actions takes nothing returns nothing
    call DisableTrigger(udg_trigger18)
    call AddSpecialEffectLocBJ(GetRectCenter(udg_rect57), "Abilities\\Spells\\Other\\Awaken\\Awaken.mdl")
    call ConditionalTriggerExecute(udg_trigger_DestroyFX)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=8
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call ClearSelectionForPlayer(Player(- 1 + ( bj_forLoopAIndex )))
        set bj_forLoopAIndex=1
        set bj_forLoopAIndexEnd=8
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            if ( GetPlayerSlotState(Player(- 1 + ( bj_forLoopAIndex ))) == PLAYER_SLOT_STATE_PLAYING ) then
                call CreateNUnitsAtLoc(1, 'ewsp', Player(- 1 + ( bj_forLoopAIndex )), GetRectCenter(udg_rect57), bj_UNIT_FACING)
            endif
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
        call SelectUnitForPlayerSingle(bj_lastCreatedUnit, Player(- 1 + ( bj_forLoopAIndex )))
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=8
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call AdjustPlayerStateBJ(1, Player(- 1 + ( bj_forLoopAIndex )), PLAYER_STATE_RESOURCE_LUMBER)
        call AdjustPlayerStateBJ(150, Player(- 1 + ( bj_forLoopAIndex )), PLAYER_STATE_RESOURCE_GOLD)
        call CreateFogModifierRectBJ(true, Player(- 1 + ( bj_forLoopAIndex )), FOG_OF_WAR_VISIBLE, bj_mapInitialPlayableArea)
        set udg_fogmodifiers01[bj_forLoopAIndex]=bj_lastCreatedFogModifier
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call SetPlayerFlagBJ(PLAYER_STATE_GIVES_BOUNTY, true, Player(10))
    call SetPlayerFlagBJ(PLAYER_STATE_GIVES_BOUNTY, true, Player(11))
    set udg_booleans04[1]=false
    set udg_booleans04[2]=false
    if ( CountUnitsInGroup(GetUnitsInRectMatching(udg_rect57, Condition(function Trig_Init_STARTGAME_FilterTeam1))) > 0 ) then
        set udg_booleans04[1]=true
    endif
    if ( CountUnitsInGroup(GetUnitsInRectMatching(udg_rect57, Condition(function Trig_Init_STARTGAME_FilterTeam2))) > 0 ) then
        set udg_booleans04[2]=true
    endif
    call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS, 8., "|cff33cc88          To select a hero attack him with your wisp|r")
    call ConditionalTriggerExecute(udg_trigger22)
    call PolledWait(1.)
    call EnableTrigger(udg_trigger111)
    call PolledWait(10.)
    call ConditionalTriggerExecute(udg_trigger171)
endfunction

