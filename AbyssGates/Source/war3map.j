    return ( IsUnitIllusion(GetTriggerUnit()) == false ) and ( GetUnitTypeId(GetTriggerUnit()) != 'h012' ) and ( GetItemType(GetManipulatedItem()) == ITEM_TYPE_ARTIFACT ) and ( ( udg_itemDatabaseType[R2I(GetWidgetLife(GetManipulatedItem()))] == 1 ) or ( udg_itemDatabaseType[R2I(GetWidgetLife(GetManipulatedItem()))] == 2 ) or ( udg_itemDatabaseType[R2I(GetWidgetLife(GetManipulatedItem()))] == 3 ) ) and ( GetUnitTypeId(GetTriggerUnit()) != 'u005' ) // The creature
endfunction

function Trig_Acquire_Weapon1_Actions takes nothing returns nothing
 local unit u= GetTriggerUnit()
    local player owner= GetOwningPlayer(u)
 local item itm= GetManipulatedItem()
    set udg_integer02=R2I(GetWidgetLife(itm))
    if ( udg_HeroPrimaryHandSlot[( 1 + GetPlayerId(owner) )] ) then // Already using primary hand
        call PolledWait(.05)
        if ( GetItemUserData(itm) != 0 ) then // Already existed.
            call CreateItemLoc(GetItemTypeId(itm), GetUnitLoc(u))
            call SetWidgetLife(itm, .0)
            call RemoveItem(itm)
            call DisplayTimedTextToForce(GetForceOfPlayer(owner), 3., "Weapon Slot in Use - Item Dropped")
        else
            call CreateItemLoc(GetItemTypeId(itm), GetUnitLoc(u)) // New item.
            set udg_item01=bj_lastCreatedItem
            call SetWidgetLife(itm, .0)
            call RemoveItem(itm)
            call AdjustPlayerStateBJ(( udg_itemDatabaseGold[udg_integer02] / 10 ), owner, PLAYER_STATE_RESOURCE_GOLD)
            call DisplayTimedTextToForce(GetForceOfPlayer(owner), 3., "Weapon Slot in Use - Item sold for 100% of cost")
            call StartSoundForPlayerBJ(owner, gg_snd_Error)
            set udg_TargetUnit=u
            call ConditionalTriggerExecute(udg_trigger53) // ItemSell Trigger
        endif
        call StartSoundForPlayerBJ(owner, gg_snd_Error)
    else
        call DisplayTimedTextToForce(GetForceOfPlayer(owner), 3., "1-Handed Weapon Acquired")
        set udg_HeroPrimaryHandSlot[( 1 + GetPlayerId(owner) )]=true
        
        call SetItemUserData(itm, 1)
    endif
    if ( GetPlayerState(owner, PLAYER_STATE_RESOURCE_LUMBER) == 0 ) then
        call UnitRemoveAbility(u, 'A001')
        call UnitRemoveAbility(u, 'A01Y')
        call UnitAddAbility(u, 'A001')
    endif
endfunction

