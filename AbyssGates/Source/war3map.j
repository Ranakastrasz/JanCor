function Trig_Tele_Actions takes nothing returns nothing
    local unit u= GetTriggerUnit()
 local real targetX
 local real targetY
 local region trigReg= GetTriggeringRegion()
    local real sourceX= GetUnitX(u)
    local real sourceY= GetUnitY(u)
 local rect targReg
    if ( CloseToRect(udg_rect53 , sourceX , sourceY) or CloseToRect(udg_rect55 , sourceX , sourceY) ) then //Top Home Target
        set targReg=udg_rect05
    elseif ( CloseToRect(udg_rect47 , sourceX , sourceY) or CloseToRect(udg_rect45 , sourceX , sourceY) ) then //Bottom Home Target
        set targReg=udg_rect06
    elseif ( CloseToRect(udg_rect43 , sourceX , sourceY) ) then
        set targReg=udg_rect28
    elseif ( CloseToRect(udg_rect44 , sourceX , sourceY) ) then
        set targReg=udg_rect29
    elseif ( CloseToRect(udg_rect51 , sourceX , sourceY) ) then
        set targReg=udg_rect30
    elseif ( CloseToRect(udg_rect52 , sourceX , sourceY) ) then
        set targReg=udg_rect31
    elseif ( CloseToRect(udg_rect46 , sourceX , sourceY) or CloseToRect(udg_rect42 , sourceX , sourceY) ) then
        set targReg=udg_rect32
    elseif ( CloseToRect(udg_rect48 , sourceX , sourceY) or CloseToRect(udg_rect41 , sourceX , sourceY) ) then
        set targReg=udg_rect33
    elseif ( CloseToRect(udg_rect54 , sourceX , sourceY) or CloseToRect(udg_rect49 , sourceX , sourceY) ) then
        set targReg=udg_rect34
    elseif ( CloseToRect(udg_rect56 , sourceX , sourceY) or CloseToRect(udg_rect50 , sourceX , sourceY) ) then
        set targReg=udg_rect35
    endif
    set targetX=GetRectCenterX(targReg)
    set targetY=GetRectCenterY(targReg)
    call AddSpecialEffect("Abilities\\Spells\\Human\\Polymorph\\PolyMorphDoneGround.mdl", GetUnitX(u), GetUnitY(u))
    call ConditionalTriggerExecute(udg_trigger_DestroyFX)
    call SetUnitPosition(GetTriggerUnit(), targetX, targetY)
    if ( IsUnitType(u, UNIT_TYPE_HERO) ) then
        call PanCameraToForPlayer(GetOwningPlayer(u), targetX, targetY)
    else
        //Do not pan camera for summoned units, only for hero.
    endif
    call AddSpecialEffect("Abilities\\Spells\\Human\\MassTeleport\\MassTeleportCaster.mdl", GetUnitX(u), GetUnitY(u))
    call ConditionalTriggerExecute(udg_trigger_DestroyFX)
endfunction

